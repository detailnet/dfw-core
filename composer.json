{
    "name": "detailnet/dfw-core",
    "description": "DETAIL Framework common library",
    "type": "library",
    "keywords": [
        "dfw",
        "detailnet"
    ],
    "homepage": "https://github.com/detailnet/dfw-core/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Wolf",
            "email": "ivan.wolf@detailnet.ch"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-inputfilter": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "doctrine/orm": "~2.4.6",
        "ocramius/proxy-manager": "~0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3.5",
        "squizlabs/php_codesniffer" : "~1.5.5",
        "satooshi/php-coveralls": "dev-master",
        "phpmd/phpmd": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Detail\\Core\\": "src/",
            "DetailTest\\Core\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml",
            ".coveralls.yml",
            ".travis.yml",
            "build.xml",
            "build.properties",
            "composer.phar"
        ]
    }
}
